<bl:fragment xmlns:bl="http://www.bluexin.be/com/saomc/saoui/fragment-schema"
             name="jump bar fragment">
    <texture>minecraft:textures/gui/icons.png</texture>
    <children>
        <glRectangle name="bg">
            <srcY cache="STATIC">84</srcY>
            <w cache="STATIC">182</w>
            <h cache="STATIC">5</h>
        </glRectangle>
        <glRectangle name="fill">
            <enabled>horsejump > 0</enabled>
            <srcY cache="STATIC">89</srcY>
            <w>182 * horsejump</w>
            <h cache="STATIC">5</h>
        </glRectangle>
    </children>
</bl:fragment>